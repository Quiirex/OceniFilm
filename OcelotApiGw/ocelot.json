{
  "Routes": [
    {
      "DownstreamPathTemplate": "/identiteta",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 80 // TODO Check
        }
      ],
      "UpstreamPathTemplate": "/api/identiteta",
      "UpstreamHttpMethod": [ "Get" ],
